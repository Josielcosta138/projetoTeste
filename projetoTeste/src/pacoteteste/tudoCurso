package pacoteteste;

import java.util.Scanner;

import java.io.*;

public class estruturaDeclaracao {

	public static Scanner input = new Scanner(System.in);

	// Método execultavel do Java
	public static void main(String[] args) throws IOException {

		double quantidadeLitrosViagem, consumoPorLitro, tempoViagem, velociadeMedia;
		double distancia, litrosUsado, precoCombustivel, valorCombustivel;
		double quantidadePedagio;
		double valorPedagio, pedagioGasolina, gastoPedagio;
		String nome;

		consumoPorLitro = 12;
		valorPedagio = 3.96;

		System.out.println(" -- Programa Computador de Bordo --");

		System.out.println("\n Informe seu nome ? ");
		nome = input.next();

		System.out.print("\n Informe o tempo de sua Viagem ? : ");
		tempoViagem = input.nextDouble();

		System.out.print("\n Informe a velocidade Média de sua Viagem ? : ");
		velociadeMedia = input.nextDouble();

		System.out.print("\n Informe Preço do Combustivel ? : ");
		precoCombustivel = input.nextDouble();

		System.out.print("\n Informe Quantidade de Pedágios ? : ");
		quantidadePedagio = input.nextDouble();

			distancia = tempoViagem * velociadeMedia;

			quantidadeLitrosViagem = distancia / consumoPorLitro;

			valorCombustivel = precoCombustivel * quantidadeLitrosViagem;

			gastoPedagio = valorPedagio * quantidadePedagio;

			pedagioGasolina = gastoPedagio + valorCombustivel;
			
			
			
			
			System.out.println("----------------------------------------------------");
			System.out.println(" ");
			System.out.println(" ---> Informações do Computador de Bordo<--- \n");
			System.out.println(" ");
			System.out.println(" Escolha um tipo de vizualização ? \n");
			System.out.println(" ");
			System.out.print("\n Individual ou Parcial ? ");
			System.out.println(" ");
			System.out.print("\n 1° Individual. ");
			System.out.print("\n 2° Parcial. ");
			System.out.print("\n 3° Sair. ");
			System.out.println(" ");
			
			int opcaoVisualizacao  = input.nextInt();
			
			int opcao = 0;
			
			switch (opcaoVisualizacao) {
			
			case 1:
				System.out.println(" Vizualizção Individual ");
				System.out.println(" ");
				System.out.println(" ---> Informações do Computador de Bordo<--- \n");
				System.out.println(" ");
				System.out.println(" Escolha uma opção ");
				System.out.println(" ");
				System.out.println(" ");
				System.out.print("\n 1° Tempo Viagem. ");
				System.out.print("\n 2° Velocidade Média. ");
				System.out.print("\n 3° Distância. ");
				System.out.print("\n 4° Quantidade de Litros gastos. ");
				System.out.print("\n 5° Gastos Comustivel Viagem. ");
				System.out.print("\n 6° Gastos Pedágio. ");
				System.out.print("\n 7° Gastos Pedágio E Gasolina. ");
				System.out.print("\n 8° Sair. ");
				System.out.println(" ");
				System.out.println(" ");
				
				
									
				while (opcao != 8) {
					
					opcao = input.nextInt();
					
				switch (opcao) {
				
				case 1:
					System.out.println(" Condutor :"+nome);
					System.out.println(" ");
					System.out.println(" Tempo Viagem "+tempoViagem);
					break;
					
				case 2:
					System.out.println(" Condutor :"+nome);
					System.out.println(" ");
					System.out.println(" Velocidade Média "+velociadeMedia);
					break;
					
				case 3:
					System.out.println(" Condutor :"+nome);
					System.out.println(" ");
					System.out.println(" Distância "+distancia);
					break;
					
				case 4:
					System.out.println(" Condutor :"+nome);
					System.out.println(" ");
					System.out.println(" Quantidade de Litros gastos "+quantidadeLitrosViagem);
					break;
					
				case 5:
					System.out.println(" Condutor :"+nome);
					System.out.println(" ");
					System.out.println(" Gastos Comustivel Viagem "+valorCombustivel);
					break;
					
				case 6:
					System.out.println(" Condutor :"+nome);
					System.out.println(" ");
					System.out.println(" Gastos Pedágio "+gastoPedagio);
					break;
					
				case 7:
					System.out.println(" Condutor :"+nome);
					System.out.println(" ");
					System.out.println(" Gastos Pedágio E Gasolina "+pedagioGasolina);
					break;
				
				case 8:
					System.out.println(" Sair....");
					break;
					
				default:
					System.out.println(" Opção Invalida! ");
					break;
				}
			}
			
			break;
				
			case 2:
			
				System.out.println("----------------------------------------------------");
				System.out.println(" ");
				System.out.println(" Vizualização Parcial ");
				System.out.println(" ");
				System.out.println(" ---> Informações do Computador de Bordo<--- \n");
				System.out.println(" ");
				System.out.println(" Condutor : "+nome+" .");
				System.out.println(" ");
				System.out.println(" ");
				System.out.println(" Tempo de Viagem: "+tempoViagem+" Hras");
				System.out.println(" ");
				System.out.println(" Velocidade Média: "+velociadeMedia+" Km/h");
				System.out.println(" ");
				System.out.println(" Distancia: "+distancia+" Km");
				System.out.println(" ");
				System.out.println(" Quantidade de Litros gastos: "+quantidadeLitrosViagem+" L");
				System.out.println(" ");
				System.out.println(" Gastos Comustivel Viagem: "+valorCombustivel+" R$");
				System.out.println(" ");
	            System.out.println(" Gastos Pedágio: "+gastoPedagio+" Pdg");
	            System.out.println(" ");
	            System.out.println(" Gastos Pedágio E Gasolina: "+pedagioGasolina+" Pdg");
	            System.out.println(" ");
	            System.out.println("------------------------------------------------------------|");
					
				break;
				
			case 3:
				System.out.println(" Sair...");
				
				break;
				
			default:
				System.out.println(" OPÇÃO INVALIDA ");
					
				break;
			}
			
			
		}
}



// Método execultavel do Java
	public static void main(String[] args) {

		// Estrutura repetição FOR com Break E Continue e Verificação de IF e Else e OU ||
		// Encotrar um dado em uma variavel dentro do For ou Lista

		int num1If = 21;
		int num2If = 48;
		int num3If = 39;
		
		System.out.println(" 1° Teste For: ");
		
		for (int numero = 5; numero <= 99; numero++) {
			System.out.println(" Num: "+numero);
			
			if (numero == num1If || numero == num3If) {
				System.out.println("   " );
				System.out.println(" Teste If 1° Encontrado e Continuo! \n Num: " + num1If);
				continue;
				
			} else {

				if (numero == num2If) {
					System.out.println("   " );
					System.out.println(" Teste If 2° Encontrado! \n Num: " + num2If);
					
					break;
				}

			}

		}






// Método execultavel do Java
	public static void main(String[] args) {

		// Estrutura repetição FOR com Parada e Verificação de IF e Else
		// Encotrar um dado em uma variavel dentro do For ou Lista

		int num1If = 27;
		int num2If = 24;

		for (int numero = 5; numero <= 99; numero++) {
			System.out.println(" Num: "+numero);
			

			if (numero == num1If) {
				System.out.println("   " );
				System.out.println(" Teste If 1 Encontrado! \n Num: " + num1If);
				break;
			} else {

				if (numero == num2If) {
					System.out.println("   " );
					System.out.println(" Teste If 2 Encontrado! \n Num: " + num2If);
					
					break;
				}




//Estrutura While - Do while Enquanto

		int contador = 1;
		String nome = "joao";
		String nome1 = "pedro";
		
		while (contador <= 8 ) {   	// Verifica depois Executa
			
			System.out.println(" Contador: "+contador);
			//System.out.println(" Nome: "+nome+"-"+nome1);
			
			contador++;
			
			
		}
		
		int contador1 = 1;
		
		do {				// Executa depois Executa
						
			System.out.println(" Contador: "+contador1);
			contador1 ++;
		} while (contador1 <= 5);



//Operadores Relacionais.
		int valor1 = 49;
		int valor2 = 29;
		int valor3 = 49;
		
		
		if (valor1 == valor2) {
			System.out.println(" Valores diferentes."+valor1+"-"+valor2);
			
		} else {
			if (valor1 != valor3) {
				System.out.println(" Valores iguais"+valor1+"-"+valor3);
			}else {
				System.out.println(" Duas verificações diferentes! ");
			}
			

		}




/*SWITCH CASE: OPERAÇOES EXATAS*/
		// Para escolhas,tipos 'MENUS'
		
		int doca = 5;

		int box1 = 38;
		int box2 = 42;
		int box3 = 342;
		int box4 = 43;

		  
		   int totalBox;
		
		   totalBox = (box1 + box2 + box3 + box4) * 4^2;
		   
		switch (doca) {
		case 1:
			System.out.println(" Box 1º armazenado: "+box1+" Itens.");
			break;
		case 2:
			System.out.println(" Box 2º armazenado: "+box2+" Itens.");
			break;	
		case 3:
			System.out.println(" Box 3º armazenado: "+box3+" Itens.");
			break;	
		case 4:
			System.out.println(" Box 4º armazenado: "+box4+" Itens.");
			break;	
		case 5:
			System.out.println(" Total de itens no estoque: "+totalBox);
			
			break;

		default:
			System.out.println(" Box Invalido!: ");
			break;
			
		}



// Operações Aninhadas: Operações dentro de operações.
		mediaAluno = (nota1 + nota2 + nota3 + nota4) / 4;
		
		
			if (mediaAluno >= 5) {
				if (mediaAluno >= 7) {
					System.out.println(" Aprovado: "+mediaAluno);
					if (mediaAluno>= 9) {
						System.out.println(" Parabéns Acima da Média: "+mediaAluno);
						
					}
				}else {
					System.out.println(" Caiu na recuperação ");
					System.out.println(" ");
					System.out.println(" Recuperação: "+mediaAluno);
					System.out.println(" ----------------------------- ");
					System.out.println(" ");
					
					nota1 = 6; nota2 = 7; nota3 = 8 ; nota4 = 7;		// Refazendo notas e Média
					mediaAluno = (nota1 + nota2 + nota3 + nota4) / 4;
					
					if (mediaAluno >= media) {					// Média entrando na Condição
						System.out.println(" Passou na recuperação: "+mediaAluno);
						
					} else {
						System.out.println(" Reprovou na recuperação: "+mediaAluno);

					}
				}
			} else {
				System.out.println(" Reprovado: "+mediaAluno);

			}





/* Condições lógicas com IF e ELSE
		
		
			System.out.println(" Media: "+mediaAluno);

		  if (mediaAluno >= media) {
			  System.out.println(" Aluno aprovado: "+mediaAluno);
			  
			  
		  }else if (mediaAluno < media && mediaAluno >= 4) {
				System.out.println(" Fazer recuperação: "+mediaAluno);
				
			}
			   else {
			      System.out.println(" Aluno reprovado: "+mediaAluno);
				
			}

		}*/	






/* Operadores Ternários ' Para micro Validações */
		
		mediaAluno = (nota1 + nota2 + nota3 + nota4) / 4;
		
		String saidaResultado = mediaAluno >= media? " Aluno aprovado." : (mediaAluno < media && mediaAluno >= 4)?  
				"Aluno recuperação." : " Aluno reprovado.";
		System.out.println(saidaResultado + " - media: "+ mediaAluno);




